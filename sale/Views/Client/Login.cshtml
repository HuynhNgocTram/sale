
@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>

<form method="post" action="/Client/DoneLogin">
    <div class="form-horizontal">
        <div class="form-group">
            <label class="control-label col-md-2">UserID</label>
            <div class="col-md-2">
                <input type="text" name="userid" value="eee"/>
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-2">Password</label>
            <div class="col-md-2">
                <input type="password" name="pass" value="123"/>
            </div>
        </div>
    </div>
    <button type="submit">Login</button>
</form>
